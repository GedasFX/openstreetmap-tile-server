Map {
  background-color: @land-color;
}

@water-color: @s-w;
@land-color: @s-l;

@s-b: #444; // stl-building-color
@s-l: #222; // stl-land-color
@s-w: #000; // stl-water-color
@s-r: #555; // stl-road-color

@s-t: #888; // stl-text-color
@s-to: #000; // stl-text-outline-color
@s-i: #aaa; // stl-icon-color

@invisible: rgba(0, 0, 0, 0); 


@standard-halo-radius: 1;
@standard-halo-fill: @s-to;